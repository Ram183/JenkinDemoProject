pipeline {
    agent any
     stages {
        stage('Build') {
            steps {
                echo "Building Stage"
                bat "mvn clean"
            }
        }
    }
     stages {
        stage('Test') {
            steps {
                echo "Testing Stage"
                 bat "mvn test"
            }
        }
    }
    stages {
        stage('compile') {
            steps {
                echo "Compiling the Project"
                bat "mvn compile"
            }
        }
    }
       stages {
        stage('Deploy') {
            steps {
                echo "Deploying Stage"
            }
        }
    }
}
